          <field name="HIGHLIGHT_MODE">javascript</field>
          <field name="TEXT">$('.wall', parent.document).click((e) =&gt; {&#10;    try {&#10;        let el = $(e.target).parents('.option');&#10;        let val = $(el).data('value').split(',');&#10;        PROC['changeFrontWall'](val[0].toUpperCase() + '.jpg');&#10;        PROC['changeSideWall'](val[val.length - 1].toUpperCase() + '.jpg');&#10;        $('.wall', parent.document).removeClass('active');&#10;        $(el).addClass('active');&#10;        &#10;    } catch (err) {&#10;        &#10;    }&#10;    &#10;})&#10;&#10;$('.floor', parent.document).click((e) =&gt; {&#10;    let el = $(e.target).parents('.option');&#10;    let val = $(el).data('value');&#10;    PROC['changeFloor'](val);&#10;    $(el).addClass('active').siblings().removeClass('active')&#10;})&#10;&#10;$('.shelf', parent.document).click((e) =&gt; {&#10;    let el = $(e.target).parents('.option');&#10;    let val = $(el).data('value');&#10;    PROC['changeShelf'](val);&#10;    $(el).addClass('active').siblings().removeClass('active')&#10;})&#10;&#10;$('.bath', parent.document).click((e) =&gt; {&#10;    let el = $(e.target).parents('.option');&#10;    let val = $(el).data('value');&#10;    PROC['changeBath'](val);&#10;    $(el).addClass('active').siblings().removeClass('active')&#10;})</field>
        </shadow>
      </value>
    </block>
